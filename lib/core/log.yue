import 'ffi' as :C
import RL from yuema

{
	ALL:     C.LOG_ALL
	TRACE:   C.LOG_TRACE
	DEBUG:   C.LOG_DEBUG
	INFO:    C.LOG_INFO
	WARNING: C.LOG_WARNING
	ERROR:   C.LOG_ERROR
	FATAL:   C.LOG_FATAL
	NONE:    C.LOG_NONE

	trace: (logLevel, text, ...) -> RL.TraceLog(logLevel, text, ...)
	setLevel: (logLevel) -> RL.SetTraceLogLevel(logLevel)
}
